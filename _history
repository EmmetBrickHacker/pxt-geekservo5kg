void {\n        let buf = pins.createBuffer(4);\n        if (servo == 0) {\n            buf[0] = 0x03;\n        }\n        if (servo == 1) {\n            buf[0] = 0x04;\n        }\n        if (servo == 2) {\n            buf[0] = 0x05;\n        }\n        if (servo == 3) {\n            buf[0] = 0x06;\n        }\n        if (servo == 4) {\n            buf[0] = 0x07;\n        }\n        if (servo == 5) {\n            buf[0] = 0x08;\n        }\n        if (servo == 6) {\n            buf[0] = 0x09;\n        }\n        if (servo == 7) {\n            buf[0] = 0x10;\n        }\n\n        angle = Math.map(angle, 0, 360, 0, 180)\n        \n        buf[1] = angle;\n        buf[2] = 0;\n        buf[3] = 0;\n        pins.i2cWriteBuffer(board_address, buf);\n    }\n\n    //% weight=71\n    //% blockId=setServoSpeed block=\"Set GeekServo 5kg %servo speed to %speed\\\\%\"\n    //% speed.min=-100 speed.max=100\n    export function set5kgServoSpeed(servo: ServoList, speed: number): void {\n        speed = Math.map(speed, -100, 100, 0, 180)\n        setServoAngle(servo, speed)\n    }\n}\n","pxt.json":"{\n    \"name\": \"motor\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\",\n        \"wuKong\": \"github:elecfreaks/pxt-wukong#v1.5.6\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\",\n        \"GeekServo5kg.ts\"\n    ],\n    \"preferredEditor\": \"blocksprj\"\n}\n"}},{"timestamp":1743888689251,"editorVersion":"7.0.61","text":{"main.blocks":"<xml xmlns=\"https://developers.google.com/blockly/xml\"><variables><variable id=\"oj@xVQv-$RkY1$FANPGB\">pulse</variable><variable id=\"~tCc8k9d-J98~n!EIffl\">speed</variable><variable id=\"`l-UOO|Sk[8m?jX05O/q\">angle</variable></variables><block type=\"device_button_event\" x=\"967\" y=\"0\"><field name=\"NAME\">Button.B</field><statement name=\"HANDLER\"><block type=\"variables_set\"><field name=\"VAR\" id=\